<?xml version="1.0" encoding="utf-8"?>
<animated-selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:id="@+id/hey_normal_pick_focused"
        android:state_focused="true"
        android:drawable="@drawable/hey_picker_nomral_picked" />

    <item
        android:id="@+id/hey_normal_pick_unfocused"
        android:state_focused="false"
        android:drawable="@drawable/hey_picker_nomral_unpicked"/>

    <transition
        android:drawable="@drawable/hey_picker_normal_cancel"
        android:fromId="@+id/hey_normal_pick_focused"
        android:toId="@+id/hey_normal_pick_unfocused" />

    <transition
        android:drawable="@drawable/hey_picker_normal_choose"
        android:fromId="@+id/hey_normal_pick_unfocused"
        android:toId="@+id/hey_normal_pick_focused" />
</animated-selector>