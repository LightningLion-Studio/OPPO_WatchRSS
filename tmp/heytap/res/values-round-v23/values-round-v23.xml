    <style name="HeyArrowStyle">
        <item name="android:visibility">gone</item>
    </style>
    <style name="HeyBackTitleStyle" parent="HeyTitleTextStyle">
        <item name="android:textStyle">bold</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:maxWidth">@dimen/hey_title_bar_max_width</item>
    </style>
    <style name="HeyBasePickerStyle">
        <item name="android:layout_height">@dimen/hey_picker_height</item>
        <item name="heyTextSizeSelected">@dimen/hey_picker_selected_text_size</item>
        <item name="heyTextSizeNormal">@dimen/hey_picker_default_text_size</item>
        <item name="heyFontFamily">"oplus-sans-medium"</item>
    </style>
    <style name="HeyCancelTitleStyle" parent="HeyTitleTextStyle">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textStyle">bold</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:textColor">@color/hey_colorAccent</item>
    </style>
    <style name="HeyCheckBoxStyle" parent="android:Widget">
        <item name="HeyCheckBoxStateList">@drawable/hey_checkbox_state</item>
        <item name="android:focusable">true</item>
        <item name="android:clickable">true</item>
        <item name="android:textAppearance">?android:attr/textAppearance</item>
        <item name="HeyCheckboxSize">@dimen/hey_listitem_widget_size</item>
        <item name="android:height">@dimen/hey_listitem_widget_size</item>
        <item name="android:width">@dimen/hey_listitem_widget_size</item>
        <item name="android:textColor">#FF000000</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
    </style>
    <style name="HeyClockStyle">
        <item name="android:textSize">@dimen/hey_m_desription</item>
        <item name="android:textColor">@color/hey_clock_color</item>
        <item name="android:textStyle">bold</item>
        <item name="android:format12Hour">h:mm</item>
        <item name="android:format24Hour">HH:mm</item>
        <item name="android:letterSpacing">0.05</item>
        <item name="isArcLayout">true</item>
    </style>
    <style name="HeyEditTextStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/hey_edit_bg</item>
        <item name="android:textSize">@dimen/hey_l_desription</item>
        <item name="android:textCursorDrawable">@drawable/hey_cursor</item>
        <item name="android:fontFamily">"oplus-sans-medium"</item>
        <item name="android:paddingVertical">@dimen/hey_edittext_padding_vertical</item>
        <item name="android:paddingHorizontal">@dimen/hey_edittext_padding_horizontal</item>
        <item name="android:textColorHint">@color/hey_edit_text_hint_color</item>
        <item name="android:textColor">@color/white</item>
    </style>
    <style name="HeyGroupTitleTextStyle" parent="android:Widget.TextView">
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/hey_l_desription</item>
        <item name="android:layout_marginTop">@dimen/hey_group_title_margin_top</item>
        <item name="android:layout_marginBottom">@dimen/hey_group_title_margin_bottom</item>
        <item name="android:fontFamily">"oplus-sans-medium"</item>
        <item name="android:gravity">center_horizontal</item>
    </style>
    <style name="HeyLoadingDialogContentStyle">
        <item name="android:fontFamily">"oplus-sans-medium"</item>
    </style>
    <style name="HeyMainTitleTextStyle" parent="HeyTitleTextStyle">
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">@dimen/hey_s_title</item>
        <item name="android:ellipsize">end</item>
        <item name="android:maxWidth">@dimen/hey_title_bar_max_width</item>
    </style>
    <style name="HeyMultipleDefaultItemStyle" parent="HeySingleDefaultItemStyle">
        <item name="heySummaryText">@string/hey_multiple_item_summary_text</item>
    </style>
    <style name="HeyPageIndicatorStyle">
        <!--圆圈方向（横竖）-->
        <item name="dotOrientation">horizontal</item>
        <!--圆圈直径-->
        <item name="dotSize">@dimen/hey_pageindicator_dot_size</item>
        <!--圆圈距离底部的间距-->
        <item name="dotRoundMargin">@dimen/hey_wear_page_indicator_padding</item>
        <!--圆圈被选中的颜色-->
        <item name="traceDotColor">@color/hey_page_color_selected</item>
        <!--圆圈未选中的颜色-->
        <item name="dotColor">@color/hey_page_color</item>
        <!--圆圈之间的角度-->
        <item name="dotRoundAngle">5</item>
    </style>
    <style name="HeyRadioButtonStyle" parent="android:Widget.CompoundButton.RadioButton">
        <item name="android:button">@drawable/hey_radio_button_selector</item>
    </style>
    <style name="HeyRadioStyle" parent="HeyRadioButtonStyle">
        <item name="android:layoutDirection">rtl</item>
        <item name="android:textDirection">ltr</item>
    </style>
    <style name="HeySingleBigLeftImageItemStyle">
        <item name="android:paddingStart">@dimen/hey_item_big_left_image_padding_start</item>
        <item name="android:paddingEnd">@dimen/hey_listitem_padding_right</item>
        <item name="android:paddingTop">@dimen/hey_item_big_left_image_padding_vertical</item>
        <item name="android:paddingBottom">@dimen/hey_item_big_left_image_padding_vertical</item>
        <item name="heyLeftImageWidth">@dimen/hey_listitem_big_lefticon_height_width</item>
        <item name="heyLeftImageHeight">@dimen/hey_listitem_big_lefticon_height_width</item>
    </style>
    <style name="HeySingleDefaultItemStyle">
        <item name="heyText">@string/hey_default_item_text</item>
    </style>
    <style name="HeyTitleTextStyle" parent="android:Widget.TextView">
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/hey_l_desription</item>
        <item name="android:text">@string/hey_default_title_text</item>
        <item name="android:ellipsize">end</item>
        <item name="android:singleLine">true</item>
        <item name="android:fontFamily">"sys-sans-en"</item>
        <item name="android:layout_marginTop">@dimen/hey_title_margin_vertical_top</item>
        <item name="android:layout_marginBottom">@dimen/hey_title_margin_vertical</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="HeyUIRadioStyle" parent="HeyRadioButtonStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/hey_l_desription</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:fontFamily">"oplus-sans-medium"</item>
        <item name="android:paddingHorizontal">@dimen/hey_content_horizontal_distance_6_0</item>
        <item name="android:paddingVertical">@dimen/hey_ui_radio_padding_vertical</item>
        <item name="android:lineSpacingMultiplier">1.03</item>
        <item name="android:layoutDirection">rtl</item>
        <item name="android:textDirection">ltr</item>
        <item name="android:background">@drawable/hey_base_item_bg</item>
    </style>
